
<div class="jumbotron">
  <h1>gCamp</h1>
  <p>Your life, organized.</p>
  <p></p>
</div>


<br/>
<div class="row">
  <div class="col-md-4">
    <%= image_tag "holycrap.jpg", class: "img-responsive" %>
    <br/>
    <div class="panel panel-primary">
      <div class="panel-heading">Tasks</div>
      <div class="panel-body">
        <p>Grouped by projects and lists. Just the way you like'em</p>
        <br/>
        <br/>
      </div>
    </div>

  </div>
  <div class="col-md-4">
    <%= image_tag "people.jpg", class: "img-responsive" %>
    <br/>
    <div class="panel panel-primary">
      <div class="panel-heading">Documents</div>
      <div class="panel-body">
        <ul>
          <li>Upload</li>
          <li>Comment</li>
          <li>Revise</li>
          <br/>
        </ul>
      </div>
    </div>
  </div>
  <div class="col-md-4">
    <%= image_tag "crowd.jpg", class: "img-responsive" %>
    <br/>
    <div class="panel panel-primary">
      <div class="panel-heading">Comments</div>
      <div class="panel-body">
        <ul>
          <li>Comment on tasks and documents</li>
          <li>Get email notifications</li>
          <br/>
          <br/>
        </ul>
      </div>
    </div>
  </div>
</div>
<blockquote>
  <% @quotes.transpose.each do |quote| %>
  <p>
    <%= quote[0] %>
  </p>
  <p>
    <em><%= quote[1] %></em>
  </p>
  <% end %>
</blockquote>
